{
    "name": "privacy-ontology-gui",
    "version": "10.1.0",
    "description": "Angular 12 with Electron (Typescript + SASS + Hot Reload)",
    "homepage": "https://github.com/emiltang/PrivacyOntologyGui",
    "author": {
        "name": "Emil Tang Kristensen",
        "email": "emiltangkristensengmail.com"
    },
    "keywords": [
        "angular",
        "angular 12",
        "electron",
        "nodejs",
        "typescript",
        "spectron",
        "eslint",
        "sass",
        "windows",
        "mac",
        "linux"
    ],
    "main": "app/main.js",
    "private": true,
    "scripts": {
        "postinstall": "electron-builder install-app-deps",
        "ng": "ng",
        "start": "npm-run-all -p electron:serve ng:serve",
        "build": "npm run electron:serve-tsc && ng build --base-href ./",
        "build:dev": "npm run build -- -c dev",
        "build:prod": "npm run build -- -c production",
        "ng:serve": "ng serve -c web -o",
        "electron:serve-tsc": "tsc -p tsconfig.serve.json",
        "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve --remote-debugging-port=9223",
        "electron:local": "npm run build:prod && npx electron .",
        "electron:build": "npm run build:prod && electron-builder build --publish=never",
        "test": "ng test --watch=false",
        "test:watch": "ng test",
        "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
        "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
        "lint": "ng lint"
    },
    "dependencies": {
        "@angular/animations": "13.0.1",
        "@angular/cdk": "13.0.1",
        "@angular/common": "13.0.1",
        "@angular/compiler": "13.0.1",
        "@angular/core": "13.0.1",
        "@angular/flex-layout": "12.0.0-beta.35",
        "@angular/forms": "13.0.1",
        "@angular/material": "13.0.1",
        "@angular/platform-browser": "13.0.1",
        "@angular/platform-browser-dynamic": "13.0.1",
        "@angular/router": "13.0.1",
        "@comunica/actor-init-sparql": "^1.22.1",
        "@fontsource/roboto": "4.5.0",
        "@swimlane/ngx-charts": "19.1.0",
        "ansi-regex": "6.0.1",
        "events": "3.3.0",
        "material-design-icons": "3.0.1",
        "rxjs": "^7.3.0",
        "stream": "0.0.2",
        "stream-browserify": "3.0.0",
        "string-width": "5.0.1",
        "strip-ansi": "7.0.1",
        "tslib": "^2.1.0",
        "uuid": "8.3.2",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-builders/custom-webpack": "13.0.0",
        "@angular-devkit/build-angular": "13.0.2",
        "@angular-eslint/builder": "12.6.1",
        "@angular-eslint/eslint-plugin": "12.6.1",
        "@angular-eslint/eslint-plugin-template": "12.6.1",
        "@angular-eslint/schematics": "12.4.1",
        "@angular-eslint/template-parser": "12.6.1",
        "@angular/cli": "13.0.2",
        "@angular/compiler-cli": "13.0.1",
        "@angular/language-service": "13.0.1",
        "@ngx-translate/core": "14.0.0",
        "@ngx-translate/http-loader": "7.0.0",
        "@types/jasmine": "3.10.2",
        "@types/jasminewd2": "2.0.10",
        "@types/mocha": "9.0.0",
        "@types/node": "16.11.7",
        "@typescript-eslint/eslint-plugin": "4.28.3",
        "@typescript-eslint/parser": "5.3.1",
        "chai": "4.3.4",
        "conventional-changelog-cli": "2.1.1",
        "cross-env": "7.0.3",
        "electron-builder": "22.13.1",
        "electron-reload": "2.0.0-alpha.1",
        "eslint": "8.2.0",
        "eslint-plugin-import": "2.25.3",
        "eslint-plugin-jsdoc": "37.0.3",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "jasmine-core": "3.10.1",
        "jasmine-spec-reporter": "7.0.0",
        "karma": "6.3.8",
        "karma-coverage-istanbul-reporter": "3.0.3",
        "karma-electron": "7.1.0",
        "karma-jasmine": "4.0.1",
        "karma-jasmine-html-reporter": "1.7.0",
        "mocha": "9.1.3",
        "nan": "2.15.0",
        "npm-run-all": "4.1.5",
        "spectron": "15.0.0",
        "ts-node": "10.4.0",
        "typescript": "~4.4.4",
        "wait-on": "6.0.0",
        "webdriver-manager": "12.1.8"
    },
    "engines": {
        "node": ">=14.0.0"
    },
    "browserslist": [
        "chrome 91"
    ]
}
